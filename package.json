{
    "name": "template-next-ts",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "eslint . --fix --max-warnings=0",
        "test": "jest",
        "typecheck": "tsc --project tsconfig.json --noEmit",
        "test:watch": "jest --coverage --watch",
        "test:failure": "jest -f",
        "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
        "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
        "generate": "yarn plop --plopfile generators/plopfile.js",
        "test:coverage": "jest --coverage",
        "prepare": "husky install",
        "storybook": "start-storybook -s ./public -p 6006",
        "build-storybook": "build-storybook -s ./public"
    },
    "dependencies": {
        "@apollo/client": "^3.8.1",
        "@next/font": "^13.1.1",
        "@styled-icons/boxicons-regular": "^10.47.0",
        "@styled-icons/fa-brands": "^10.47.0",
        "@types/lodash.xor": "^4.5.7",
        "@types/styled-components": "^5.1.26",
        "apollo": "^2.34.0",
        "axios": "^1.2.1",
        "babel-plugin-styled-components": "^2.0.7",
        "dotenv": "^16.3.1",
        "graphql": "^16.8.0",
        "jest-styled-components": "^7.1.1",
        "joi": "^17.10.1",
        "lodash.xor": "^4.5.0",
        "next": "13.1.1",
        "next-auth": "^4.23.1",
        "next-pwa": "^5.6.0",
        "nextjs-progressbar": "^0.0.16",
        "node-fetch": "^3.3.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-slick": "^0.29.0",
        "styled-components": "^5.3.6",
        "styled-icons": "^10.47.0",
        "styled-media-query": "^2.1.2"
    },
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@commitlint/cli": "^17.3.0",
        "@commitlint/config-conventional": "^17.3.0",
        "@storybook/addon-actions": "^6.5.15",
        "@storybook/addon-essentials": "^6.5.15",
        "@storybook/addon-interactions": "^6.5.15",
        "@storybook/addon-links": "^6.5.15",
        "@storybook/builder-webpack5": "^6.5.15",
        "@storybook/manager-webpack5": "^6.5.15",
        "@storybook/react": "^6.5.15",
        "@storybook/testing-library": "^0.0.13",
        "@swc/core": "^1.3.37",
        "@swc/jest": "^0.2.24",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/react-hooks": "^8.0.1",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest": "^29.2.4",
        "@types/node": "18.11.18",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.10",
        "@types/react-slick": "^0.23.10",
        "@types/testing-library__jest-dom": "^5.14.5",
        "@typescript-eslint/eslint-plugin": "^5.47.1",
        "@typescript-eslint/parser": "^5.47.1",
        "babel-loader": "^8.3.0",
        "eslint": "8.30.0",
